{
  "manifest_version": 2,
  "name": "Focus",
  "version": "1.0",
  "description": "A Chrome app to help you stay off the YouTubes.",
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/*",
        "https://www.facebook.com/*",
        "https://www.reddit.com/*",
        "https://www.twitter.com/*"
      ],
      "js": ["./main.js"]
    }
  ],
  "page_action": {
    "default_popup": "./popup/popup.html",
    "default_icon": {
      "16": "images/img16.png",
      "32": "images/img32.png",
      "48": "images/img192.png",
      "128": "images/img192.png"
    }
  },
  "icons": {
    "16": "images/img16.png",
    "32": "images/img32.png",
    "48": "images/img192.png",
    "128": "images/img192.png"
  }
}
